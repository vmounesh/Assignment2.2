
public class printXpattern {
	
	public static void main(String args[])
	{
		
		for(int i=1;i<=5;i++)
		{
			if(i==1)
			{
				System.out.println("*________*");
		
			}
			else if(i==2)
			{
				System.out.println("__*___*   ");
			}
			else if(i==3)
			{
				System.out.println("_____*   ");
			}
			else if(i==4)
			{
				System.out.println("__*___*  ");
			}
			else
			{
				System.out.println("*________*");
				
			}	
		}
	}
	

}
